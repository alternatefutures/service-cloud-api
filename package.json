{
  "name": "@alternatefutures/backend",
  "version": "0.1.2",
  "description": "AlternateFutures GraphQL Backend API",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "dev:runtime": "tsx watch src/runtime/server.ts",
    "start:runtime": "node dist/runtime/server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio --port 1611",
    "db:seed": "tsx prisma/seed.ts",
    "generate:invoice": "tsx scripts/generate-sample-invoice.ts",
    "billing:run-now": "tsx scripts/run-compute-billing-now.ts",
    "billing:backfill": "tsx scripts/backfill-compute-billing.ts",
    "generate:types": "graphql-codegen --config codegen.ts",
    "generate:types:watch": "graphql-codegen --config codegen.ts --watch",
    "deploy:railway": "./deploy-railway.sh",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,json,md}\"",
    "prepare": "node -e \"try{require('husky')}catch{}\""
  },
  "dependencies": {
    "@akashnetwork/chain-sdk": "1.0.0-alpha.0",
    "@ardrive/turbo-sdk": "^1.33.0",
    "@clickhouse/client": "^1.8.1",
    "@cosmjs/proto-signing": "^0.38.1",
    "@cosmjs/stargate": "^0.38.1",
    "@graphql-tools/schema": "^10.0.29",
    "@infisical/sdk": "^4.0.6",
    "@lighthouse-web3/sdk": "^0.4.3",
    "@prisma/client": "^6.19.0",
    "@solana/web3.js": "^1.98.4",
    "@types/node-cron": "^3.0.11",
    "acme-client": "^5.4.0",
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.3",
    "ethers": "^6.15.0",
    "graphql": "^16.12.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-query-complexity": "^1.1.0",
    "graphql-validation-complexity": "^0.4.2",
    "graphql-yoga": "^5.16.2",
    "helmet": "^8.1.0",
    "http-proxy": "^1.18.1",
    "ipfs-http-client": "^60.0.1",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^4.2.1",
    "pdfkit": "^0.17.2",
    "stripe": "^19.3.1",
    "svg-to-pdfkit": "^0.1.8",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@graphql-codegen/cli": "^6.1.0",
    "@graphql-codegen/schema-ast": "^5.0.0",
    "@graphql-codegen/typescript": "^5.0.5",
    "@graphql-codegen/typescript-operations": "^5.0.5",
    "@graphql-codegen/typescript-resolvers": "^5.1.3",
    "@noble/hashes": "^2.0.1",
    "@scure/bip32": "^2.0.1",
    "@scure/bip39": "^2.0.1",
    "@types/bcrypt": "^6.0.0",
    "@types/graphql-depth-limit": "^1.1.6",
    "@types/http-proxy": "^1.17.17",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^24.10.1",
    "@types/pdfkit": "^0.17.3",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.46.4",
    "@vitest/coverage-v8": "^4.0.13",
    "bip39": "^3.1.0",
    "blakejs": "^1.2.1",
    "ed25519-hd-key": "^1.3.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.6.2",
    "prisma": "^6.19.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.1"
  },
  "engines": {
    "node": ">=18"
  },
  "pnpm": {
    "overrides": {
      "axios": ">=1.13.5",
      "elliptic": "^6.6.1",
      "secp256k1": "^5.0.1",
      "parse-duration": "^2.1.3",
      "ws": "^8.18.3",
      "nanoid": "^5.0.9",
      "fast-xml-parser": ">=4.5.4",
      "hono": ">=4.11.4",
      "preact": ">=10.28.2",
      "h3": ">=1.15.5",
      "qs": ">=6.14.2",
      "minimatch": ">=9.0.7"
    }
  },
  "packageManager": "pnpm@10.22.0+sha512.bf049efe995b28f527fd2b41ae0474ce29186f7edcb3bf545087bd61fbbebb2bf75362d1307fda09c2d288e1e499787ac12d4fcb617a974718a6051f2eee741c"
}
